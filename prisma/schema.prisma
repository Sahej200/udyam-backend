generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Submission {
  id            Int      @id @default(autoincrement())
  hasAadhaar    String?
  aadhaarNumber String?
  name          String?
  otp           String?
  panNumber     String?
  createdAt     DateTime @default(now())
}